{
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-vue-urql": "^2.3.3",
    "@headlessui/vue": "^1.7.3",
    "@iconify-json/fa6-brands": "^1.1.6",
    "@iconify-json/fa6-regular": "^1.1.6",
    "@iconify-json/fa6-solid": "^1.1.7",
    "@iconify-json/twemoji": "^1.1.5",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.8.3",
    "@urql/exchange-retry": "^1.0.0",
    "@urql/vue": "^1.0.2",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.3.0",
    "@vueuse/integrations": "^9.3.0",
    "@vueuse/motion": "^2.0.0-beta.12",
    "@vueuse/router": "^9.3.0",
    "autoprefixer": "^10.4.12",
    "core-js": "^3.25.5",
    "daisyui": "^2.31.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.11",
    "unplugin-vue-components": "^0.22.8",
    "unplugin-vue-router": "^0.2.3",
    "vite": "^3.1.6",
    "vite-plugin-graphql-codegen": "^2.3.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-vue": "^9.6.0",
    "prettier": "^2.7.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/eslint-config-typescript/recommended",
      "@vue/eslint-config-prettier",
      "plugin:tailwindcss/recommended",
      "./.eslintrc-auto-import.json"
    ],
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".git",
      "*.d.ts",
      "*.config.ts",
      "*.config.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "newline-per-chained-call": "off",
      "tailwindcss/classnames-order": "off",
      "tailwindcss/enforces-negative-arbitrary-values": "error",
      "tailwindcss/enforces-shorthand": "error",
      "tailwindcss/migration-from-tailwind-2": "error",
      "tailwindcss/no-arbitrary-value": "off",
      "tailwindcss/no-contradicting-classname": "error",
      "tailwindcss/no-custom-classname": "off",
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/multi-word-component-names": "off",
      "vue/no-v-html": "off",
      "vue/singleline-html-element-content-newline": "off"
    }
  },
  "name": "spectrograph-frontend",
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "tailwindcss": {}
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm"
  },
  "version": "1.0.0"
}
